        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@ckeditor/ckeditor5-build-classic": "^11.0.1",
        "ajv": "^6.5.3",
        "imagemin": "^5.3.1",
        "img-loader": "^3.0.0",
        "install-peerdeps": "^1.8.0",
        "npm-install-peers": "^1.2.1",
        "select2": "^4.0.6-rc.1",
        "slugify": "^1.3.1"
